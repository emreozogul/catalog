<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="484.0" prefWidth="594.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.t4.catalog.EditController">
   <children>
      <TableView fx:id="tableView" editable="true" layoutX="-7.0" prefHeight="484.0" prefWidth="326.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Attribute Name" />
          <TableColumn fx:id="valueColumn" prefWidth="75.0" text="Attribute Value" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar layoutX="381.0" layoutY="430.0" prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save" />
            <Button fx:id="CancelButton" mnemonicParsing="false" onAction="#CancelButtonClicked" text="Cancel" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
